# KEKTECH 3.0 Prediction Markets - Environment Variables Template
# Copy this file to .env and fill in your values

# Network RPC URLs
BASED_TESTNET_RPC=https://testnet-rpc.based.network
BASED_MAINNET_RPC=https://mainnet-rpc.based.network

# Private key for deployment (DO NOT COMMIT YOUR ACTUAL PRIVATE KEY!)
PRIVATE_KEY=your_private_key_here

# Block explorer API key for contract verification
EXPLORER_API_KEY=your_explorer_api_key_here

# Gas reporting (set to "true" to enable gas cost reporting)
REPORT_GAS=false
